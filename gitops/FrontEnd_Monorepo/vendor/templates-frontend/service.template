apiVersion: v1
kind: Service
metadata:
  name: frontend-$SERVICE_NAME
  namespace: $BRANCHE_NAME
spec:
  ports:
    - port: 80
      targetPort: $TARGET_PORT
      name: api
    - port: 3001
      targetPort: 3001
      name: websocket      
  selector:
    app.kubernetes.io/name: frontend-$SERVICE_NAME